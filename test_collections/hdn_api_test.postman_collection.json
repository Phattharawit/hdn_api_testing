						"exec": [
							"pm.test(\"GetMessageList_Nofilter_ReturnOk\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"GetMessageList_Nofilter_ResponseLessThan200ms\", function () {\r",
