						"exec": [
							"pm.test(\"DeleteMessage_UnProcessedLxMessage_ReturnConflict\", function () {\r",
							"    pm.response.to.have.status(409);\r",
							"});\r",
							"\r",
							"pm.test(\"DeleteMessage_UnProcessedLxMessage_ReturnErrorMessage\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    var fileName = pm.environment.get(\"UnProcessedLxFileName\");\r",
							"    pm.expect(jsonData.Data.ExceptionMessage).to.equal(\"Unprocessed LX Message cannot be deleted : \" + fileName);  \r",
							"});\r",
