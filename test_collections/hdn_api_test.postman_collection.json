					"script": {
						"id": "57951f60-2374-4eed-a521-805fc087a3b7",
						"exec": [
							"pm.test(\"GetMessage_InValidFileName_ReturnNotFound\", function () {\r",
							"    pm.response.to.have.status(404);\r",
							"});\r",
							"\r",
							"pm.test(\"GetMessage_InValidFileName_ReturnErrorMessage\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    var fileName = pm.environment.get(\"notExitingFileName\");\r",
							"    pm.expect(jsonData.Data.ExceptionMessage).to.equal(\"File: \" + fileName + \" not found.\");  \r",
							"});\r",
							" "
