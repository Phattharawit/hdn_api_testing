				{
					"listen": "test",
					"script": {
						"id": "d8f0e238-e17d-44b4-907b-2e73a5ff0c66",
						"exec": [
							"\r",
							"pm.test(\"GetMessage_ValidFileName_ReturnOk\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"GetMessage_ValidFileName_ReturnJsonCorrectFormat\", function () {\r",
							"    \r",
							"    var jsonData = pm.response.json();\r",
							"    var responseModel = jsonData.Data;\r",
							"    \r",
							"    //expected model shape\r",
							"    var model = {\r",
							"        Action: null,\r",
							"        Base64String: null,\r",
							"        FileName: null,\r",
							"        FileType: null,\r",
							"        MessageType: null\r",
							"    }\r",
							"    \r",
							"    for (var propName in responseModel) {\r",
							"        //assert response model spec are identical with the expected model shape\r",
							"       pm.expect(model.hasOwnProperty(propName)).to.equal(true);  \r",
							"    }\r",
							"}); "
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "bcf6b486-667c-438c-97cb-d816675115cb",
						"exec": [
